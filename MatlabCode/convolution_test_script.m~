% Script to test associativity of convolution in 3 dimensions
A = zeros(5,5,7);
B = zeros(3,3);
C = zeros(4,1);
test1 = zeros(5,5,7);
test2 = zeros(5,5,7);

for i = 1:size(A,1)
    for j = 1:size(A,2)
        for k = 1:size(A,3)
            A(i,j,k) = random('unif',1,10);
        end
    end
end

for i = 1:size(B,1)
    for j = 1:size(B,2)
        B(i,j) = random('unif',1,10);
    end
end

for i = 1:size(C,1)
    C(i) = random('unif',1,10);
end

% test sequence
for i = 1:size(A,3)
    test1(:,:,i) = conv2(A(:,:,i),size
end